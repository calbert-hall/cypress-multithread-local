{
  "name": "tutorial-cypress",
  "version": "1.0.0",
  "description": "This contains Applitools Cypress tutorial",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --browser chrome --spec 'cypress/integration/examples/ultraFastGrid.spec.js'",
    "test:ci": "cypress run --browser chrome --headless --spec 'cypress/integration/examples/ultraFastGrid.spec.js'",
    "cypress:open": "cypress open",
    "cy:run": "cypress run",
    "cy:parallel": "cypress-parallel -s cy:run -t 2 -d 'cypress/integration/examples/' -a '\"--env FIXED_APPLITOOLS_BATCH_ID=testBatchId \"' "
  },
  "author": "Applitools Team <team@applitools.com> (http://www.applitools.com/)",
  "license": "ISC",
  "dependencies": {
    "@applitools/eyes-cypress": "^3.23.3",
    "cypress": "^9.1.0",
    "cypress-parallel": "^0.9.0"
  }
}
